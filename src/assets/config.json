        "state-lifecycle": {
          "data-file": "w3c/state-lifecycle.xml"
        },
        "conditional-rendering": {
          "data-file": "w3c/conditional-rendering.xml"
        }
      }
    },
    "react": {
      "topics": {
        "templates": {
          "data-file": "react/templates.xml"
        },
        "components": {
          "data-file": "react/components.xml"
        },
        "state-lifecycle": {
          "data-file": "react/state-lifecycle.xml"
        },
        "conditional-rendering": {
          "data-file": "react/conditional-rendering.xml"
        }
      }
    },
    "vue": {
      "topics": {
        "templates": {
          "data-file": "vue/templates.xml"
        },
        "components": {
          "data-file": "vue/components.xml"
        },
        "state-lifecycle": {
          "data-file": "vue/state-lifecycle.xml"
        },
        "conditional-rendering": {
          "data-file": "vue/conditional-rendering.xml"
        }
      }
    }
