  "sidebar": {
    "links": {
      "html-basics": { "text": "HTML basics", "url": "./html-basics" },
      "templates": { "text": "Templates", "url": "./templates" },
      "components": { "text": "Components", "url": "./components" },
      "state-lifecycle": { "text": "State and lifecycle", "url": "./state-lifecycle" },
      "conditional-rendering": { "text": "Conditional rendering", "url": "./conditional-rendering" }
    }
  },
  "framework-picker": {
    "default": "w3c",
    "buttons": {
