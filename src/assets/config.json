  "router": {
    "default": "/html-basics",
    "routes": {
      "html-basics": { "url": "/html-basics" },
      "templates": { "url": "/templates" },
      "components": { "url": "/components" },
      "state-lifecycle": { "url": "/state-lifecycle" },
